{
    "compilerOptions": {
      "target": "esnext",
      "useDefineForClassFields": true,
      "lib": ["dom", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "module": "esnext",
      "jsx": "react"
    },
    "include": ["src"]
  }
  